{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)",
    "^react-redux(.*)",
    "react-router-dom",
    "^@reduxjs(.*)",
    "<THIRD_PARTY_MODULES>",
    "mui/",
    "routes/",
    "assets/",
    "api/",
    "store/",
    "contexts/",
    "hooks/",
    "types/",
    "constants/",
    "helpers/",
    "utils/",
    "pages/",
    "layouts/",
    "components/",
    "styles/",
    "^./(.(?!.*.css$))",
    "^../(.*)",
    ".css"
  ],
  "importOrderSortSpecifiers": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 120
}
